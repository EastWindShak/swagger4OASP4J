<?xml version="1.0" encoding="UTF-8"?>
<access-control-schema>
  <group id="ReadMasterData" type="group">
    <permissions>
      <!-- BEGIN ARCHETYPE SKIP -->
      <!-- samplemanagement -->
      <permission id="FindSample"/>
      <!-- moremanagement -->
      <permission id="FindMore"/>
      <!-- END ARCHETYPE SKIP -->
    </permissions>
  </group>

  <!-- BEGIN ARCHETYPE SKIP -->
  <group id="Admin" type="role">
    <inherits>
      <group-ref>ReadMasterData</group-ref>
    </inherits>
    <permissions>
      <!-- samplemanagement -->
      <permission id="SaveSample"/>
      <permission id="DeleteSample"/>
    </permissions>
  </group>
  <!-- END ARCHETYPE SKIP -->
</access-control-schema>